<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zq.mapper.DeptMapper">
<sql id="baseSql">
    id,name,age,db_source,create_time
</sql>

<select id="getByIdAge" resultType="com.zq.entity.Dept">
    select <include refid="baseSql"/>
    from dept
    where id = #{id}
    and age = #{age}
</select>
</mapper>
